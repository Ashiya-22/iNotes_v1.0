<div class="w-[15.8%] mx-auto mt-14">
    <nav class="flex px-5 py-3 text-gray-700 border border-gray-200 rounded-lg bg-gray-50 dark:bg-gray-800 dark:border-gray-700 mb-10" aria-label="Breadcrumb">
        <ol class="inline-flex items-center space-x-1 md:space-x-2 rtl:space-x-reverse">
          <li class="inline-flex items-center">
            <a href="/dashboard" class="inline-flex items-center text-sm font-medium text-gray-700 hover:text-blue-600 dark:text-gray-400 dark:hover:text-white">
              <svg class="w-3 h-3 me-2.5" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 20 20">
                <path d="m19.707 9.293-2-2-7-7a1 1 0 0 0-1.414 0l-7 7-2 2a1 1 0 0 0 1.414 1.414L2 10.414V18a2 2 0 0 0 2 2h3a1 1 0 0 0 1-1v-4a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1v4a1 1 0 0 0 1 1h3a2 2 0 0 0 2-2v-7.586l.293.293a1 1 0 0 0 1.414-1.414Z"/>
              </svg>
              Dashboard
            </a>
          </li>
          <li aria-current="page">
            <div class="flex items-center">
              <svg class="rtl:rotate-180  w-3 h-3 mx-1 text-gray-400" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 6 10">
                <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m1 9 4-4-4-4"/>
              </svg>
              <span class="ms-1 text-sm font-medium text-gray-500 md:ms-2 dark:text-gray-400">Search</span>
            </div>
          </li>
        </ol>
      </nav>
</div>

<div class="bg-cyan-700 w-[60%] h-fit min-h-[40vh] mx-auto rounded-lg border-2 border-gray-400 mt-10 mb-20 py-12
flex flex-col items-center">
<div class="mt-2 mb-2 tracking-tight leading-none text-white sm:text-2xl md:text-3xl lg:text-5xl self-center">Results</div>
<div class="h-[4px] w-[4%] bg-[#50C878] self-center rounded-full mb-16 move"></div>
<div class="flex flex-col text-center">
    <% if(searchResults != '') { %>
      <% searchResults.forEach(function (note, index) { %>
        
        <a href="/dashboard/item/<%= note._id %>">
        <h2 class="tracking-tight leading-none underline mb-2 text-yellow-500 sm:text-2xl md:text-3xl lg:text-3xl"><%= note.title %></h2>
        </a>

      <% }) %>
    <% } else { %>
        <div class=" w-full h-full flex flex-col">
            <img src="/assets/smiling-face-with-tear-svgrepo-com.svg" alt="Let's Start Img" class="w-[11%] self-center">
            <div class=" my-8 tracking-tight leading-none text-white sm:text-2xl md:text-2xl lg:text-4xl self-center">Nothing here :(</div>
        </div> 
    <% } %>
 </div>
</div>