<div class="flex w-[90%] mx-auto mt-24 mb-8 justify-between">
    <div class="font-semibold tracking-tight leading-none text-white sm:text-2xl md:text-3xl lg:text-4xl">Welcome, <%=displayName%></div>
    <button type="button" class="focus:outline-none text-white bg-green-700 hover:bg-green-800 focus:ring-4 focus:ring-green-300 font-medium rounded-lg text-sm px-5 py-2.5 me-2 mb-2 dark:bg-green-600 dark:hover:bg-green-700 dark:focus:ring-green-800"><a href="/dashboard/add">+ Add Note</a></button>
</div>
<div class="bg-cyan-700 w-[90%] h-fit mx-auto rounded-lg border-2 border-gray-400 mb-24 p-10
flex flex-col">
    <div class="mt-8 mb-4 tracking-tight leading-none text-white sm:text-2xl md:text-3xl lg:text-5xl self-center">Your Notes</div>
    <div class="h-[4px] w-[4%] bg-[#50C878] self-center rounded-full mb-16 move"></div>

    <div class="flex flex-wrap justify-center gap-y-16 gap-x-24">
        <% if (notes.length > 0) { for(var i=0; i < notes.length; i++) { %>
            
        <a href="/dashboard/item/<%= notes[i]._id %>" class="block max-w-sm p-10 bg-white border border-gray-200 rounded-lg shadow hover:bg-gray-100 dark:bg-gray-800 dark:border-gray-700 dark:hover:bg-gray-700 min-w-[35%]">
        <h5 class="mb-2 text-2xl font-bold tracking-tight text-gray-900 dark:text-white"><%= notes[i].title %></h5>
        <p class="font-normal text-gray-700 dark:text-gray-400"><%= notes[i].body %></p>
        </a>
    <% }} else { %> 
        <div class=" w-full h-full flex flex-col">
            <img src="/assets/notes-notebook-svgrepo-com.svg" alt="Let's Start Img" class="w-[12%] self-center">
            <div class=" my-8 tracking-tight leading-none text-white sm:text-2xl md:text-2xl lg:text-4xl self-center">Let's get started ! Click <a href="/dashboard/add" class="text-yellow-400 underline">here</a> to add your first note :)</div>
        </div> 
    <% }%>
    </div>

    <% if(notes.length > 0) { %>
        <nav aria-label="Page navigation example" class="self-center mt-20 mb-4">
            <ul class="flex items-center -space-x-px h-8 text-sm">
            <% if(current == 1) { %>
                <li class="disabled">
                    <a class="flex items-center justify-center px-3 h-8 leading-tight text-gray-500 bg-white border border-gray-300 hover:bg-gray-100 hover:text-gray-700 dark:bg-gray-800 dark:border-gray-700 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white rounded-l-lg">First</a>
                </li>
            <% } else { %>
                <li>
                    <a href="/dashboard" class="flex items-center justify-center px-3 h-8 leading-tight text-gray-500 bg-white border border-gray-300 hover:bg-gray-100 hover:text-gray-700 dark:bg-gray-800 dark:border-gray-700 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white rounded-l-lg">First</a>
                </li>
            <% }  %>
            <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
            <% if (i !== 1 ) { %>
                <li>
                    <a href="#" class="flex items-center justify-center px-3 h-8 leading-tight text-gray-500 bg-white border border-gray-300 hover:bg-gray-100 hover:text-gray-700 dark:bg-gray-800 dark:border-gray-700 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white">...</a>
                </li>
            <% } %>
            <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
              <% if(i == current ) { %>
                <li>
                    <a href="#" class="flex items-center justify-center px-3 h-8 leading-tight text-gray-500 bg-white border border-gray-300 hover:bg-gray-100 hover:text-gray-700 dark:bg-gray-800 dark:border-gray-700 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white"><%= i %></a>
                </li>
              <% } else { %>
                <li>
                    <a href="/dashboard/?page=<%= i %>" class="flex items-center justify-center px-3 h-8 leading-tight text-gray-500 bg-white border border-gray-300 hover:bg-gray-100 hover:text-gray-700 dark:bg-gray-800 dark:border-gray-700 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white"><%= i %></a>
                </li>
              <% } %>
              <% if(i == Number(current) + 4 && i < pages) { %>
                <li>
                    <a href="#" class="flex items-center justify-center px-3 h-8 leading-tight text-gray-500 bg-white border border-gray-300 hover:bg-gray-100 hover:text-gray-700 dark:bg-gray-800 dark:border-gray-700 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white">...</a>
                </li>
              <% } %>
            <% } %>
            <% if (current == pages) { %>
                <li class="disabled">
                    <a class="flex items-center justify-center px-3 h-8 leading-tight text-gray-500 bg-white border border-gray-300 hover:bg-gray-100 hover:text-gray-700 dark:bg-gray-800 dark:border-gray-700 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white rounded-r-lg">Last</a>
                </li>
            <% } else { %>
                <li>
                    <a href="/dashboard/?page=<%= pages %>" class="flex items-center justify-center px-3 h-8 leading-tight text-gray-500 bg-white border border-gray-300 hover:bg-gray-100 hover:text-gray-700 dark:bg-gray-800 dark:border-gray-700 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white rounded-r-lg">Last</a>
                </li>
            <% } %>
          </ul>
        </nav>
    
      <% } %>


</div>